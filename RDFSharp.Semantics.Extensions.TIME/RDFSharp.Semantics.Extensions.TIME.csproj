<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyTitle>RDFSharp.Semantics.Extensions.TIME</AssemblyTitle>
    <AssemblyName>RDFSharp.Semantics.Extensions.TIME</AssemblyName>
    <AssemblyVersion>$(Version)</AssemblyVersion>
    <Version>3.4.0</Version>
    <Authors>Marco De Salvo</Authors>
    <Copyright>Marco De Salvo</Copyright>
    <Description>RDFSharp.Semantics extensions for modeling and querying temporal data</Description>
    <TargetFramework>netstandard2.0</TargetFramework>
    <SignAssembly>false</SignAssembly>
    <Product>RDFSharp.Semantics</Product>
    <PackageId>RDFSharp.Semantics.Extensions.TIME</PackageId>
    <PackageVersion>$(Version)</PackageVersion>
    <PackageTags>rdfs owl owl-dl owl2 time ontology reasoner swrl semantic-web linked-data</PackageTags>
    <PackageProjectUrl>https://github.com/mdesalvo/RDFSharp.Semantics.Extensions</PackageProjectUrl>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/mdesalvo/RDFSharp.Semantics.Extensions.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Debug'">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <OutputPath>bin\Debug\</OutputPath>
    <DocumentationFile>bin\Debug\$(TargetFramework)\RDFSharp.Semantics.Extensions.TIME.xml</DocumentationFile>
    <WarningLevel>4</WarningLevel>
    <DebugType>full</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <Optimize>false</Optimize>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <DefineConstants>TRACE</DefineConstants>
    <OutputPath>bin\Release\</OutputPath>
    <DocumentationFile>bin\Release\$(TargetFramework)\RDFSharp.Semantics.Extensions.TIME.xml</DocumentationFile>
    <WarningLevel>0</WarningLevel>
    <DebugType>portable</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <Optimize>true</Optimize>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="NodaTime" Version="3.1.6" />
    <PackageReference Include="RDFSharp" Version="3.4.0" />
	<PackageReference Include="RDFSharp.Semantics" Version="3.4.0" />
  </ItemGroup>
</Project>